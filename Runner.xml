        <parameter name="udid" value="[udid value]"/>
        <parameter name="platformName" value="Android"/>
        <parameter name="platformVersion" value="9"/>
        <parameter name="browserName" value="Chrome"/>
        <!--parameter name="appType" value="browser"/-->
        <parameter name="appType" value="installedApp"/>
        <!--parameter name="appType" value="APK"/-->
